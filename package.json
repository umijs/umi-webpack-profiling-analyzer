{
  "name": "umi-webpack-profiling-analyzer",
  "version": "0.0.0-development",
  "description": "Webpack plugin and CLI utility that represents module compilation stats",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack",
    "lint": "eslint --ext js,jsx .",
    "test": "nyc --reporter=lcov mocha --exit --require @babel/register --coverage",
    "test-dev": "mocha --watch --require @babel/register",
    "dev": "./node_modules/.bin/webpack-dev-server --config test/dev-server/webpack.config.js",
    "build": "npx babel src -d lib --extensions \".ts\"",
    "watch": "npx babel src --watch -d lib --extensions \".ts\"",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umijs/umi-webpack-profiling-analyzer.git"
  },
  "keywords": [
    "webpack",
    "analyze",
    "compilation",
    "stats"
  ],
  "author": "RaoHai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umijs/umi-webpack-profiling-analyzer/issues"
  },
  "homepage": "https://github.com/umijs/umi-webpack-profiling-analyzer#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/polyfill": "7.4.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "7.4.3",
    "@nivo/bar": "^0.59.2",
    "@nivo/core": "^0.59.1",
    "@nivo/pie": "^0.59.1",
    "@nivo/tooltip": "^0.59.1",
    "@types/chai": "^4.2.4",
    "@types/express": "^4.17.2",
    "@types/webpack": "^4.39.8",
    "@types/ws": "^6.0.3",
    "autoprefixer": "9.5.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "8.0.5",
    "babel-plugin-lodash": "3.3.4",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "css-loader": "^3.2.0",
    "ejs": "^2.7.1",
    "eslint": "^5.16.0",
    "eslint-config-th0r": "^2.0.0",
    "eslint-config-th0r-react": "^2.0.0",
    "eslint-plugin-react": "^7.16.0",
    "express": "^4.17.1",
    "less": "^3.0.0",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "opener": "^1.5.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "semantic-release": "^15.13.27",
    "style-loader": "^1.0.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.8.2"
  }
}
